<h1>Google Books Search</h1>

<%= form_tag(results_path, method: :get) do %>
    <%= text_field_tag(:search, params[:search]) %>
    <%= submit_tag("Search") %>
<% end %>
<br>
<p><%= @test %>

<%= form_for @results do |f| %>
    <% @books %>
    <br>
    <%= f.label :title %><br>
    <br>
    <%= f.label :author %><br>
    <br>
    <%= f.label :company %><br>
<% end %>